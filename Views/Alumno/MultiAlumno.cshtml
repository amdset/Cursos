@model List<Alumno>
@{
    ViewData["Title"] = "Información de Alumnos";
    Layout="Simple";
}

<div class="text-center">
    <strong>Desde la vista normal</strong>
    <h1 class="display-4">@ViewData["Title"]</h1>
 
<strong>Desde la vista parcial</strong>
<table class="table table-dark table-hover">
<thead>
    <tr>
        <th scope="col">Nombre</th>
        <th scope="col">Id</th>
    </tr>
</thead>
<tbody>
@foreach (var alumno in Model)//Some code
{
    <tr>
        <td>@alumno.Nombre</td>
        <td>@alumno.UniqueId</td>
    </tr>
}
</tbody>
</table>

</div>
<p> @ViewBag.Fecha </p>
